{
  "name": "webpack-boilerplate",
  "version": "0.2.0",
  "description": "Webpack react+typescript boilerplate",
  "main": "src/index.tsx",
  "author": "Alexey-Koren",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alexey-Koren/webpack-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alexey-Koren/webpack-boilerplate.git"
  },
  "scripts": {
    "dev": "webpack serve --open --color --hot --mode=development",
    "prod": "cross-env NODE_ENV=production webpack serve --open --color --hot --mode=production",
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -f -r build",
    "analyze": "cross-env analyze=true NODE_ENV=production webpack --mode=production",
    "prettier:format": "prettier --write \"**/*.+(ts|tsx|scss)\"",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:fix-staged": "eslint --fix",
    "fix": "run-s prettier:format lint:fix",
    "fix-staged": "lint-staged",
    "svgo": "cross-env svgo --pretty --indent=2 --disable=removeViewBox,mergePaths --enable=inlineStyles  --config svgo-config.json $FILE",
    "svgo-all": "cross-env svgo -r -f src --pretty --indent=2 --disable=removeViewBox,mergePaths --enable=inlineStyles  --config svgo-config.json"
  },
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.1",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "glob-all": "^3.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-svg-loader": "^3.0.3",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "svgo": "^2.8.0",
    "svgo-loader": "^3.0.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
